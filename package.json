{
  "version": "2.0.2",
  "name": "go-try",
  "description": "Easy and flattened error handling for sync and async functions",
  "author": "onderonur",
  "license": "MIT",
  "homepage": "https://github.com/onderonur/go-try",
  "repository": {
    "type": "git",
    "url": "https://github.com/onderonur/go-try.git"
  },
  "bugs": {
    "url": "https://github.com/onderonur/go-try/issues"
  },
  "keywords": [
    "try",
    "try-catch",
    "async await",
    "go",
    "golang",
    "typescript",
    "error",
    "error handling"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "module": "dist/go-try.esm.js",
  "size-limit": [
    {
      "path": "dist/go-try.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/go-try.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
